<html>
<head>
	<title>Add Flight</title>
		<link rel="stylesheet" type="text/css" href="styles/bootstrap.css">

  <link rel="stylesheet" type="text/css" href="styles/addflight.css"> 
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link rel="stylesheet" href="https://cdnjs.cloudflare.com/ajax/libs/font-awesome/4.7.0/css/font-awesome.min.css">
  <link href="https://stackpath.bootstrapcdn.com/font-awesome/4.7.0/css/font-awesome.min.css" rel="stylesheet" integrity="sha384-wvfXpqpZZVQGK6TAh5PVlGOfQNHSoD2xbE+QkPxCAFlNEevoEH3Sl0sibVcOQVnN" crossorigin="anonymous">

  <script src="https://code.jquery.com/jquery-1.12.4.js"></script>
  <script src="https://code.jquery.com/ui/1.12.1/jquery-ui.js"></script>
</head>
<body>

<div class="topnav" id="myTopnav">
<div class="navbar-brand"><a href="/" id="brandPlane" class="fa fa-plane"><span style="color:#FF1493;">SkyTek</span></a></div>
 
  <a href="#Signout">Signout</a>
  <a href="#Delay Flight">Delay Flight</a>
  <a href="#Cancel Flight">Cancel Flight</a>
  <a href="#Add Flight" class="active">Add Flight</a>
  <a href="#Profile">Profile</a>

  <a href="javascript:void(0);" class="icon" onclick="myFunction()">
    <i class="fa fa-bars"></i>
  </a>
 </div>
<div>
</div>
 <p><h5>Welcome to SkyTek! World's leading place for your ease in travelling by Air.</h5></p>
</div>



<div class="container">
  <form>
    <div class="row">
      <div class="col-25">
        <label for="Airline ID">Airline ID</label>
      </div>
      <div class="col-75">
        <input type="text" id="Airline ID" name="Airline ID" placeholder="Airline ID">
      </div>
    </div>
    <div class="row">
      <div class="col-25">
        <label for="Flight Number">Flight Number</label>
      </div>
      <div class="col-75">
        <input type="text" id="Flight Number" name="Flight Number" placeholder="Flight Number">
      </div>
    </div>
    <div class="row">
      <div class="col-25">
        <label for="Source">Source</label>
      </div>
      <div class="col-75">
        <input type="text" id="Source" name="Source" placeholder="Source">
      </div>
    </div>
    <div class="row">
      <div class="col-25">
        <label for="Destination">Destination</label>
      </div>
      <div class="col-75">
      	  <input type="text" id="Destination" name="Destination" placeholder="Destination">
      </div>
    </div>
    <div class="row">
     <div class="col-25">
       <label for="Departure Time">Departure Time</label>
      </div>
     <div class="col-75">
       <input type="datetime-local" id="Departure Time" name="Departure Time" placeholder="Atime">
     </div>
    </div>
    <div class="row">
     <div class="col-25">
       <label for="BSeat">Seats In Business Class</label>
      </div>
     <div class="col-75">
       <input type="text" id="BSeat" name="BSeat" placeholder="50">
     </div>
    </div>

    <div class="row">
     <div class="col-25">
       <label for="BPrice">Price</label>
      </div>
     <div class="col-75">
       <input type="text" id="BPrice" name="BPrice" placeholder="1000000">
     </div>
    </div>

    <div class="row">
     <div class="col-25">
       <label for="BSeat">Seats In Economic Class</label>
      </div>
     <div class="col-75">
       <input type="text" id="ESeat" name="ESeat" placeholder="50">
     </div>
    </div>

    <div class="row">
     <div class="col-25">
       <label for="EPrice">Price</label>
      </div>
     <div class="col-75">
       <input type="text" id="EPrice" name="EPrice" placeholder="1000000">
     </div>
    </div>
    <div class="row">
      <input type="submit" value="Request Flight For Approval">
    </div>
  </form>
</div>


<script>
function myFunction() {
  var x = document.getElementById("myTopnav");
  if (x.className === "topnav") {
    x.className += " responsive";
  } else {
    x.className = "topnav";
  }
}
</script>

</body>
</html>